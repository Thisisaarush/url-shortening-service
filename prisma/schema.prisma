generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model URL {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  url         String
  shortCode   String   @unique
  accessCount Int      @default(0)
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
